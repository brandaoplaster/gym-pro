<div class="min-h-[calc(100vh-64px)] px-32 pt-20">
  <%= render "backoffice/layouts/header",
    title: "Showing Workout Sheet",
    link_text: "Back to Workout Sheets",
    link_path: backoffice_workout_sheets_path
  %>
  <%= render @workout_sheet %>
  <%= link_to "Edit this workout sheet", edit_backoffice_workout_sheet_path(@workout_sheet), class: "mt-2 rounded-md px-3.5 py-2.5 bg-gray-100 hover:bg-gray-50 inline-block font-medium" %>
  <div class="inline-block ml-2">
    <%= button_to "Destroy this workout sheet", backoffice_workout_sheet_path(@workout_sheet), method: :delete, class: "mt-2 rounded-md px-3.5 py-2.5 text-white bg-red-600 hover:bg-red-500 font-medium" %>
  </div>
</div>
