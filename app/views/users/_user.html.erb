<div id="<%= dom_id user %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name: </strong>
    <%= user.name %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Date of Birth:</strong>
    <%= user.birth_date %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Gender:</strong>
    <%= user.gender %>
  </p>
    <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= user.status %>
  </p>
</div>
